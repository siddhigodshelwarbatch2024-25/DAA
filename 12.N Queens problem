#include <stdio.h>
#include <stdbool.h>

int count = 0;
void solve(int n, int row, bool cols[], bool diag1[], bool diag2[]){
	if (row==n){
		count++;
		return;
	}
	for (int col = 0; col < n; col++) {
		if (cols[col] || diag1[row - col + n - 1] || diag2[row+col])
			continue;

		cols[col] = true;
		diag1[row - col + n - 1] = true;
		diag2[row + col] = true;

		solve(n, row + 1, cols, diag1, diag2);

		cols[col] = false;
		diag1[row - col + n - 1] = false;
		diag2[row + col] = false;
	}
}
int main() {
	int n;
	scanf("%d", &n);

	bool cols[20] = {false};
	bool diag1[40] = {false};
	bool diag2[40] = {false};

	count = 0;
	solve(n, 0, cols, diag1, diag2);
	printf("%d\n", count);

	return 0;
}
